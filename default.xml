<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <default sync-j="4" revision="zeus"/>

  <remote fetch="https://git.yoctoproject.org/git" name="yocto"/>
  <remote fetch="https://github.com/openembedded" name="oe"/>
  <remote fetch="https://github.com/STMicroelectronics" name="st"/>
  <remote fetch="https://github.com/intel-iot-devkit" name="intel-iot"/>
  <remote fetch="https://github.com/meta-qt5" name="qt5"/>
  <remote fetch="https://github.com/meta-rust" name="rust"/>
  <remote fetch="https://github.com/bytesatwork" name="bytesatwork"/>

  <project remote="yocto"        name="poky" path="sources/poky"/>
  <project remote="oe"           name="meta-openembedded" path="sources/meta-openembedded"/>
  <project remote="intel-iot"    name="meta-iot-cloud" path="sources/meta-iot-cloud"/>
  <project remote="qt5"          name="meta-qt5" path="sources/meta-qt5"/>
  <project remote="rust"         revision="master" name="meta-rust" path="sources/meta-rust"/>
  <project remote="st"           name="meta-st-stm32mp" path="sources/meta-st-stm32mp"/>
  <project remote="bytesatwork"  name="meta-bytesatwork" path="sources/meta-bytesatwork"/>
  <project remote="bytesatwork"  name="meta-bytesatwork-st" path="sources/meta-bytesatwork-st"/>
  <project remote="bytesatwork"  name="bsp-base-st" path="sources/base">
    <copyfile dest="README.md" src="README.md"/>
    <copyfile dest="setup-environment" src="setup-environment"/>
  </project>

</manifest>
